<?xml version="1.0" ?>

<!--specifies neutrino mode or antineutrino mode-->
<mode name="nu" />

<detector name="SBND" />

<channel name="numu" unit="Reconstructed Neutrino Energy [GeV]">
        <bins
        edges=" 0 1 2 3 1000" 
            />
        <truebins min="0" max="5" nbins="50"/>
    <subchannel name="cc" plotname="#nu_{#mu} CC" osc = "22" />
</channel>

<plotpot value="1"/>

<variation_list>
      <allowlist>GENIEReWeight_ICARUS_v1_multisigma_MaCCQE</allowlist>
      <denylist>piplus_Flux</denylist>
      <denylist>piminus_Flux</denylist>
</variation_list>

<MCFile treename="recTree" filename="icarus_test.flat.caf.root" scale = "1.0" maxevents="50000" pot="1"> 
     <branch
         name="rec.slc.truth.E[Sum$((rec.slc.truth.index>=0)*Iteration$)]" 
         type="double"
         associated_subchannel="nu_SBND_numu_cc"
         oscillate = "true"
         true_param_name = "rec.slc.truth.E"
         true_L_name = "rec.slc.truth.baseline"
          additional_weight = "Sum$(rec.slc.truth.index>=0) == 1"
         />
</MCFile>


